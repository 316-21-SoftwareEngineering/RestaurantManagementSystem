<Window x:Class="RestaurantManagementSystem.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RestaurantManagementSystem"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Rectangle x:Name="Logo"
                       Fill="Aqua"
                       TextElement.FontFamily="Calibri" Width="200" Margin="0,0,200,402"/>
        <TextBlock Text="LOGO" 
                   Foreground="White" 
                   FontSize="16" 
                   FontWeight="Bold"
                   HorizontalAlignment="Left" 
                   VerticalAlignment="Top" Margin="79,2,0,0"/>
    </Grid>
</Window>
